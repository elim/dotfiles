type hub &> /dev/null && alias git=hub
alias g=git
alias gb='g browse'
alias gbr='g br'
alias gbrs='g brs'
alias gbrsr='g brsr'
alias gbrz='g browse'
alias gco='g co'
alias gdi='g di'
alias gg='g grep'
alias gl='g log --stat --show-signature'
alias glp='gl -p'
alias gpl='g pull'
alias gpr='g pr'
alias gps='g push'
alias gr='git-root'
alias gref='g ref'
alias gst='g st'
