launch_app skkserv.rb --search-word $(cat /usr/local/var/run/rskkserv.pid)
launch_app pageant --background --exec pageant $(cygpath -w ~/.ssh/id_rsa.ppk)
launch_app firefox --background 
launch_app sylpheed --background --env LC_ALL=ja_JP.sjis