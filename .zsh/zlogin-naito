# -*- mode: shell-script; coding: utf-8-unix -*-
# $Id$

if [ ! -d z: ]; then
    net use z: \\\\192.168.21.30\\censys century /user:administrator /persistent:no
fi

launch_app skkserv.rb --search-word $(cat /usr/local/var/run/rskkserv.pid)
launch_app pageant --exec pageant $(cygpath -d ~/.ssh/id_rsa.ppk)
launch_app howm-synchronizer --search-word $(cat ${HOME}/var/run/howm-synchronizer.pid)
launch_app firefox
launch_app Meadow
launch_app ipmsg
launch_app set-point --search-word SetPoint
launch_app tsktrayp --search-word TskTrayP
launch_app nettb --search-word NeTTB
launch_app filemaker-pro --search-word FileMaker \
    --exec filemaker-pro $(cygpath -d ~/startup.fp7)

